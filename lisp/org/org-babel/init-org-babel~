;-*-emacs-lisp-*-

;;; active Babel language
;; ref link: http://archive.3zso.com/archives/orgmode-babel.html
(require 'http)
(org-babel-do-load-languages
      'org-babel-load-languages
      '((emacs-lisp . t)
      	(shell . t)
	(scheme . t)
	(http . t)
        (python . t)))
